	margin-top: -1px;
}
/*============ drs search box css start =============*/
.drs_search {
	width: 50%;
	height: 32px;
	float: left;
	margin: 20px 0px 0px 20px;
	margin-left: 74px;
	position: absolute;
}
#drs_s_box {
	width: 100%;
	height: 32px;
}
#drs_s_box input[type=text] {
	width: 90%;
	height: 30px;
	float: left;
	border: none;
}
#drs_s_box input[placeholder] {
	color: #000;
	font-size: 16px;
	padding-left: 20px;
	border-radius: 50px;
}
/* ============ menu css ============*/
.menu{
	width:200px;
	height: 1000px;
	min-height: 200px;
	position: fixed;
	background-color:#fff;
	box-shadow: 2px 6px 20px #888888;
	z-index: 1;
	overflow: auto;
	margin-top:0px;
	top: 70px;
	left: 0;
}
.menu ul{
	list-style-type: none;
}
.menu li{}
.menu li a{
	color: #000;
	padding:10px 25px;
	display: block;
	text-decoration: none; 
}
.menu li a:hover{
	background-color: #efefef;
}
.icon{
	width: 15px;
	height: 15px;
	float: left;
	margin: 4px 10px 0px 0px;
}
.none{
	display: none;
}
.block{
	display: block;
}
.menu li a p{
	padding-left: 25px;
	font-size: 13px;
	color: #000;
	display: none;
}
/* =============== search box icon =========*/
.search_box_icon{
    border: #000 0px solid;
    float: right;
    padding: 25px 19px 24px 20px;
    display: block;
    cursor: pointer;
}
.search_box_icon:hover {
	background-color: #052ed1;
}
.fa-search-box {
	color:#fff;
    font-size: 150% !important;
    cursor: pointer;

}
/* ============== content_wrapper =============*/
.content_wrapper{
