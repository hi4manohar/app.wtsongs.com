*{
	margin: 0px;
	padding: 0px;
}

body {
	font-family: "Segoe UI",Arial,sans-serif !important;
	background-color: #fff;
}
/* @Common Styles */
.dotes {
    text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
    -webkit-text-overflow: ellipsis;
    -o-text-overflow: ellipsis;
    -ms-text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    display: block;
    color:white;
}
/* @Common Styles End */
.fa-remove:before,.fa-close:before,.fa-times:before{content:"\00f" !important}

.header{
	width: 100%;
	height: 70px;
	background-color: #0033ff;
	position: fixed;
	top:0;
	z-index: 1;
}

.fa_size{
	font-size: 200%!important;
	color: #fff!important;
	margin: 0px 0px;
	padding: 20px 22px 18px 17px;
	float: left;
	cursor: pointer;
}
.fa_size:hover {
	background-color: #052ed1;
}
/*================= logo css start ============ */
.logo{
	width:150px;
	height: 30px;
	background-color: ;
